{
  "name": "@tpluscode/rdf-ns-builders",
  "version": "1.1.0",
  "description": "Generated RDF/JS namespace builders for common vocabularies",
  "main": "index.js",
  "module": "index.mjs",
  "browser": "index.mjs",
  "types": "index.d.ts",
  "bin": {
    "rdf-ns-builders": "bin/index.js"
  },
  "scripts": {
    "generate": "ts-node -P tsconfig.build.json src/bin/index.ts generate -p @zazuko/rdf-vocabularies -o src",
    "test": "c8 --reporter=lcov mocha --require ts-node/register tests/index.ts",
    "prepack": "npm run build",
    "build": "run-p build:*",
    "build:cjs": "tsc",
    "build:esm": "babel src --out-dir . --extensions .ts --out-file-extension .mjs",
    "release": "changeset publish"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./loose": {
      "require": "./loose.js",
      "import": "./loose.mjs"
    }
  },
  "files": [
    "*.js",
    "*.mjs",
    "bin",
    "build",
    "vocabularies",
    "*.d.ts",
    "*.d.ts.map"
  ],
  "dependencies": {
    "@rdfjs/types": "*",
    "@rdf-esm/namespace": "^0.5.1",
    "commander": "^7.2.0",
    "fs-extra": "^10.0.0"
  },
  "peerDependencies": {
    "@zazuko/rdf-vocabularies": "*",
    "clownface": "^1",
    "ts-morph": ">=11",
    "ts-node": ">= 8",
    "safe-identifier": "^0.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.4",
    "@changesets/cli": "^2.14.1",
    "@rdfjs/data-model": "^1.1.2",
    "@tpluscode/eslint-config": "0.0.4",
    "@types/chai": "^4.2.11",
    "@types/clownface": "^0.12.7",
    "@types/mocha": "^9.1.1",
    "@types/rdf-ext": "^1.3.5",
    "@types/rdfjs__namespace": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "@zazuko/rdf-vocabularies": "2021.9.22-3",
    "babel-plugin-add-import-extension": "^1.3.1",
    "c8": "^7.11.3",
    "chai": "^4.2.0",
    "clownface": "^0.12.1",
    "husky": "^3.1.0",
    "lint-staged": "^10.0.7",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "safe-identifier": "^0.4.2",
    "standard": "^17.0.0",
    "ts-morph": "^15.1.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/rdf-ns-builders.git"
  },
  "author": "Tomasz Pluskiewicz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/rdf-ns-builders/issues"
  },
  "homepage": "https://github.com/tpluscode/rdf-ns-builders#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet"
    ]
  }
}
